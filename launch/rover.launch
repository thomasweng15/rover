<launch>
	<!-- <node name="ultrasonic" pkg="rover" type="ultrasonic.py" output="screen" /> -->
	<node name="left_encoder" pkg="rover" type="encoder.py" args="left" output="screen" />
	<node name="right_encoder" pkg="rover" type="encoder.py" args="right" output="screen" />
	<node name="motors" pkg="rover" type="motors_driver.py" output="screen" />
	<node name="odom" pkg="rover" type="odometry.py" output="screen" />
	<node name="lidar" pkg="rover" type="lidar.py" output="screen" />
	<node pkg="tf" type="static_transform_publisher" name="laser_frame_broadcaster" args="0 0 0 0 0 0 1 base_link laser_frame 100" />
</launch>
